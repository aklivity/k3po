connect "tcp://localhost:12345"
connected

write "PREPARE\n"
write "version:2.0\n"
write "name:not/existing/file\n"
write "name:org/kaazing/specification/control/assets/client\n"
write "\n"

read "ERROR\n"
read "summary:Internal error\n"
read "content-length:39\n"
read "\n"
#until issue #387 is fixed, use generic regexp
#read /Script not found: not(\\|\/)existing(\\|\/)file.rpt/
read /Script not found: not.existing.file.rpt/

write "START\n"
write "\n"

read "ERROR\n"
read "summary:Internal error\n"
read "content-length:51\n"
read "\n"
read "Script has not been prepared or is still preparing\n"

write "AWAIT\n"
write "barrier:NOTIFYING_BARRIER\n"
write "\n"

read "ERROR\n"
read "summary:Internal error\n"
read "content-length:51\n"
read "\n"
read "Script has not been prepared or is still preparing\n"

write "ABORT\n"
write "\n"

read "ERROR\n"
read "summary:Internal error\n"
read "content-length:51\n"
read "\n"
read "Script has not been prepared or is still preparing\n"

write "NOTIFY\n"
write "barrier:NOTIFYING_BARRIER\n"
write "\n"

read "ERROR\n"
read "summary:Internal error\n"
read "content-length:51\n"
read "\n"
read "Script has not been prepared or is still preparing\n"

close
